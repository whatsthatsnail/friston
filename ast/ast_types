Equality:
    X = Expression;
    Op = Token;
    Y = Expression;
Comparison:
    X = Expression;
    Op = Token;
    Y = Expression;
Addition:
    X = Expression;
    Op = Token;
    Y = Expression;
Multiplication:
    X = Expression;
    Op = Token;
    Y = Expression;
Unary:
    Op = Token;
    X = Expression;
Group:
    Left = Token;
    X = Expression;
    Right = Token;
Literal:
    X = Token;